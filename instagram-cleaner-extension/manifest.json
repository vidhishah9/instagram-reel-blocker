{
  "manifest_version": 3,
  "name": "Instagram Cleaner",
  "version": "1.0",
  "description": "Blocks Reels, Explore, and Feed on Instagram.",
  "permissions": ["scripting"],
  "host_permissions": [
    "https://www.instagram.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.instagram.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
